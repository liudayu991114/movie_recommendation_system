rmats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="title_crew" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="87"/><column index="2" value="296"/><column index="3" value="300"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="title_episode" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="87"/><column index="2" value="122"/><column index="3" value="135"/><column index="4" value="142"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="title_principals" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="87"/><column index="2" value="83"/><column index="3" value="110"/><column index="4" value="169"/><column index="5" value="300"/><column index="6" value="300"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="title_ratings" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="87"/><column index="2" value="136"/><column index="3" value="94"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">create table moviedata1 as select a.titleId, a.title, b.titleType, b.startYear, b.genres from title_akas a LEFT OUTER JOIN title_basics b ON a.titleId=b.tconst



create table moviedata2 as select m.titleId, m.title, m.titleType, m.startYear, m.genres, c.directors, c.writers from moviedata1 m LEFT OUTER JOIN title_crew c ON m.titleId=c.tconst

delete from title_principals where category != &quot;actor&quot; and category != &quot;self&quot;

create table moviedata3 as select m.titleId, m.title, m.titleType, m.startYear, m.genres, m.directors, m.writers, r.averageRating, r.numVotes from moviedata2 m LEFT OUTER JOIN title_ratings r ON m.titleId=r.tconst



delete from moviedata3 where titleType != &quot;movie&quot;



delete from moviedata3 where averageRating is NULL



create table actors as SELECT tconst, group_concat(nconst) as actor from title_principals group by tconst order by tconst ASC

create table moviedata as select m.titleId, m.title, m.titleType, m.startYear, m.genres, a.actor, m.directors, m.writers, m.averageRating, m.numVotes from moviedata3 m LEFT OUTER JOIN actors a ON m.titleId=a.tconst



UPDATE moviedata set actor='\N' where actor is NULL



</sql><current_tab id="0"/></tab_sql></sqlb_project>
